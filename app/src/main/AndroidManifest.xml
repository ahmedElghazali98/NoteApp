<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ahmedElghazali.noteapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Note App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name=".listNotes" />
        <activity android:name=".createNote" />
        <activity android:name=".Notebooks" />
        <activity android:name=".createCategory" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/dotted" />

        <activity android:name=".silder2" />
        <activity android:name=".silder1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".confirmation_msg" />
        <activity android:name=".forgot_password" />
        <activity android:name=".singin" />
        <activity android:name=".singup" />
        <activity android:name=".Splash" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>